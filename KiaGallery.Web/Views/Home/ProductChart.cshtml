                                <strong class="carttitleposition">تعداد سفارش</strong><br />
                                <strong class=" cartnumberposition" style=""> @ViewBag.OrderCount</strong>
                                <br />
                                <div class="square"><strong>&nbsp; وزن: @ViewBag.OrderWeight</strong>&nbsp;&nbsp;&nbsp;<strong> &nbsp;تعداد کالا :@ViewBag.ProductCount</strong></div>
                            </div>
                            <div class="chart text-center col-md-3 col-xs-12 col-sm-12" id="productChart"></div>
                            <div class="chart text-center col-md-3 col-xs-12 col-sm-12" id="ProductTypeChart"></div>
                            <div class="chart text-center col-md-3 col-xs-12 col-sm-12" id="BranchTypeProductChart"></div>
