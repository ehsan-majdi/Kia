<script>
    setInterval(function () { $(".highcharts-credits").remove() }, 1000)
    
    $(document).ready(function () {

        $(".plusButton").click(function () {
            $(".chartSlide").stop().slideToggle(700);
            $(".ChangeIcon").toggleClass("fa fa-plus fa fa-minus");
        });
        
        $.ajax({
            type: "GET",
            url: "/home/PostChartJson/",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {
                if (response.status == 200) {
                    Highcharts.chart('postChart', {
                        
                        chart: {
                            type: 'variwide'
                        },
                        plotOptions: {
                            series: {
                                pointPadding: 0.6,
                                groupPadding: 0.2,
                                borderWidth: 3,
                                shadow: false
                            }
                        },
                        title: {
                            text: 'تعداد پست های فرستاده شده به هر شهر'
                        },
                        
                        subtitle: {
                            text: ''
                        },

                        xAxis: {
                            type: 'category',
                            title: {
                                text: ''
                            }
                        },

                        legend: {
                            enabled: false
                        },

                        series: [{
                            name: 'Labor Costs',
                            data: response.data,
                            dataLabels: {
                                enabled: true,
                                format: '%{point.y:.0f}'
                            },
                            tooltip: {
                                pointFormat: '%: <b>{point.y}</b><br>' +
                                'تعداد: <b>{point.com}</b><br>'
                            },
                            colorByPoint: true
                        }]

                    });
                }
                else {
                    callbackAlert(response.message)
                }
            }
        });

        ///چارت محصولات برای گرم و درصد کارگاه
        $.ajax({
            type: "GET",
            url: "/home/productchartjson/",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {
                if (response.status == 200) {
                    Highcharts.chart('productChart', {
                        chart: {
                            type: 'variablepie'
                        },
                        plotOptions: {
                            variablepie: {
                                size: 150
                            }
                        },
                        title: {
                            text: 'مقدار گرم سفارش داده شده به هر کارگاه'
                        },
                        tooltip: {
                            headerFormat: '',
                            pointFormat: '<span style="color:{point.color}">\u25CF</span> <b> {point.name}</b><br/>' +
                            '%<b>{point.y}</b><br/>' + 'گرم: <b>{point.com}</b><br/>'
                        },
                        series: [{
                            minPointSize: 10,
                            innerSize: '20%',

                            zMin: 0,
                            name: 'report',
                            data: response.data
                        }]
                    });
                }
                else {
                    callbackAlert(response.message)
                }
