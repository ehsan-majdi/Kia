<style type="text/css">
    .chart {
        /*min-width: 300px;
        max-width: 444px;
        height: 300px;*/
        margin: 1em auto;
    }

    .item {
        padding: 30px 0px 30px 0;
        margin-top: 10px;
        border-radius: 5px;
        color: white !important;
        text-decoration: none !important;
        height: 150px;
    }

        .item a:hover {
            text-decoration: none;
        }

        .item p {
            font-size: 16px;
            padding: 0 10px;
        }

        .item .desc {
            font-size: 12px;
            color: #EEE;
        }

    .square {
        display: block;
        position: relative;
        text-align: center;
        background-color: rgba(0, 0, 0,0.6);
        top: 38px;
        max-width: 100%;
        border-radius: 0px 2px 4px 4px;
        height: 20px;
    }


    .item-opacity:hover {
        background-color: rgba(0, 0, 0,0.3) !important
    }

    .faplusposition {
        margin-top: 40px;
        display: inline;
        position: absolute;
        right: 50px;
    }

    .carttitleposition {
        left: calc(50% - 35px);
        top: 6px;
        width: 70px;
        font-size: 13px;
        position: absolute;
    }

    .cartnumberposition {
        left: calc(50% - 55px);
        top: 20px;
        max-width: 100%;
        font-size: 30px
    }

    button {
        margin-right: 16px;
    }
</style>

<link href="~/Content/highcharts.css" rel="stylesheet" />
<script src="~/Scripts/jquery-1.12.4.js"></script>
