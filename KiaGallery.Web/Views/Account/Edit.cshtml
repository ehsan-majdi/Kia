                $(this).toggleClass("fa-plus").toggleClass("fa-minus");
            });

            $(".open-branchesPayments").on("click", function (event) {
                event.preventDefault();
                $(".branchesPayments").slideToggle("slow");
                $(this).toggleClass("fa-plus").toggleClass("fa-minus");
            });

            $(".open-report").on("click", function (event) {
                event.preventDefault();
                $(".reports").slideToggle("slow");
                $(this).toggleClass("fa-plus").toggleClass("fa-minus");
            });

            $(".open-usableProduct").on("click", function (event) {
                event.preventDefault();
                $(".ordersUsableProduct").slideToggle("slow");
                $(this).toggleClass("fa-plus").toggleClass("fa-minus");
            });
        });

        function load() {
            if (id > 0) {
                loader(true);
                $.get("/account/load/" + id, function (response) {
                    loader(false);
                    if (response.status == 200) {
                        setEntity(response.data, "#form");
                        checkForm("#form");
                        if (response.data.fileName)
                            $("#imgUserImage").attr("src", "/upload/user/" + response.data.fileName);
                    }
                    else {
                        callbackAlert(response.message, function () {
                            document.location = "/account";
                        });
                    }
                });
            }
        }

    </script>
