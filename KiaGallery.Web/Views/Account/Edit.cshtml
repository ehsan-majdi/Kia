@section Scripts {
    <script type="text/javascript">

        var id = @(ViewBag.Id != null? ViewBag.Id : 0);

        $(document).ready(function () {
            setUpStepWizard();

            setupUpload("user", function (fileName) {
                $("#imgUserImage").attr("src", "/upload/user/" + fileName);
            });
            loadRemoteSelect("#cmbWorkshop", "/workshop/getAll", null, "id", "name", "...", function () {
                loadRemoteSelect("#cmbBranch", "/branch/getAll", null, "id", "name", "...", function () {
                    loadRemoteSelect("#cmbPrintingHouse", "/printingHouse/search", null, "id", "name", "...", function () {
                    load();
                    });
                });
            });

            $("#cmbWorkshop").on("change", function () {
                if ($(this).val() > 0) {
                    $("#cmbBranch").val($("#cmbBranch option:first").val());
                    $("#cmbPrintingHouse").val($("#cmbPrintingHouse option:first").val());
                }
            });

            $("#cmbBranch").on("change", function () {
                if ($(this).val() > 0) {
                    $("#cmbWorkshop").val($("#cmbWorkshop option:first").val());
                    $("#cmbPrintingHouse").val($("#cmbPrintingHouse option:first").val());
                }
            });

            $("#cmbPrintingHouse").on("change", function () {
                if ($(this).val() > 0) {
                    $("#cmbBranch").val($("#cmbBranch option:first").val());
                    $("#cmbWorkshop").val($("#cmbWorkshop option:first").val());
                }
            });

            $("#btnSave").on("click", function (event) {
                event.preventDefault();

                if (checkForm("#form")) {
                    var entity = getEntity("#form");
                    loader(true);
                    $.post("/account/save", entity, function (response) {
                        loader(false);
                        if (response.status == 200) {
                            callbackAlert(response.message, function () {
                                document.location = "/account";
                            });
                        }
                        else {
                            alert(response.message);
                        }
                    });
                }
            });

            $("#btnNextStep1").on("click", function (event) {
                event.preventDefault();
                if (checkForm("#form")) {
                    showStep(2);
                }
            });

            $("#btnPrevStep2").on("click", function (event) {
                event.preventDefault();
                showStep(1);
            });
            $("#btnNextStep2").on("click", function (event) {
                event.preventDefault();
                showStep(3);
            });

            $("#btnPrevStep3").on("click", function (event) {
                event.preventDefault();
                showStep(2);
            });

            $(".open-manage-order").on("click", function (event) {
                event.preventDefault();
                $(".manage-order").slideToggle("slow");
                $(this).toggleClass("fa-plus").toggleClass("fa-minus");
            });

            $(".open-manage-order-tab").on("click", function (event) {
                event.preventDefault();
                $(".manage-order-tab").slideToggle("slow");
                $(this).toggleClass("fa-plus").toggleClass("fa-minus");
            });

            $(".open-manage-order-register-tab").on("click", function (event) {
