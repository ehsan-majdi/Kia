<div id="form" class="container">
    <div class="row form-group">
        <div class="col-xs-12">
            <ul class="nav nav-pills nav-justified thumbnail setup-panel no-margin">
                <li id="headerStep1" class="active">
                    <a href="#step-1">
                        <h4 class="list-group-item-heading">
                            <i class="fa fa-user" aria-hidden="true"></i>&nbsp;
                            مشخصات کاربر
                        </h4>
                        <p class="list-group-item-text hidden-xs">اطلاعات فردی کاربر رو وارد کنید.</p>
                    </a>
                </li>
                <li id="headerStep2">
                    <a href="#step-2">
                        <h4 class="list-group-item-heading">
                            <i class="fa fa-user-circle" aria-hidden="true"></i>&nbsp;
                            تصویر
                        </h4>
                        <p class="list-group-item-text hidden-xs">بارگذاری تصویر کاربر</p>
                    </a>
                </li>
                <li id="headerStep3">
                    <a href="#step-3">
                        <h4 class="list-group-item-heading">
                            <i class="fa fa-chain-broken" aria-hidden="true"></i>&nbsp;
                            حقوق دسترسی
                        </h4>
                        <p class="list-group-item-text hidden-xs">دسترسی کاربر به بخش های برنامه</p>
                    </a>
                </li>
            </ul>
        </div>
    </div>
    <div class="row setup-content" id="step-1">
        <div class="col-xs-12">
            <div class="col-md-12 well">
                <div id="step1" class="col-md-4 col-md-offset-4">
                    <div class="form-group">
                        <label for="cmbUserType">نوع کاربر</label>
                        <select id="cmbUserType" class="form-control" name="userType">
                            <option value="">...</option>
                            <option value="0">کاربر سیستم</option>
                            <option value="1">کاربر شعبه</option>
                            <option value="2">کاربر کارگاه</option>
                            <option value="3">خریدار خرجکار</option>
                            <option value="4">کاربر چاپخانه</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="cmbWorkshop">کارگاه</label>
                        <select id="cmbWorkshop" class="form-control" name="workshopId">
                            <option value="">...</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="cmbBranch">شعبه</label>
                        <select id="cmbBranch" class="form-control" name="branchId">
                            <option value="">...</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="cmbPrintingHouse">چاپخانه</label>
                        <select id="cmbPrintingHouse" class="form-control" name="printingHouseId">
                            <option value="">...</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="txtFirstName">نام به فارسی</label>
                        <input id="txtFirstName" type="text" class="form-control" name="firstName" maxlength="50" data-validate="required, maxLength(50), persianText">
                        <input type="hidden" value="" name="id" />
                    </div>
                    <div class="form-group">
                        <label for="txtLastName">نام خانوادگی به فارسی</label>
                        <input id="txtLastName" type="text" class="form-control" name="lastName" maxlength="50" data-validate="required, maxLength(50), persianText">
                    </div>
                    <div class="form-group">
                        <label for="txtUsername">نام کاربری به انگلیسی</label>
                        <input id="txtUsername" type="text" class="form-control" name="username" maxlength="50" data-validate="required, maxLength(50), username">
                    </div>
                    @if (ViewBag.Id == null)
                    {
                        <div class="form-group">
                            <label for="txtPassowrd">گذرواژه</label>
                            <input id="txtPassowrd" type="password" class="form-control" name="password" maxlength="50" data-validate="required, length(6-50)">
                        </div>
                        <div class="form-group">
                            <label for="txtConfirmPassowrd">تکرار گذرواژه</label>
                            <input id="txtConfirmPassowrd" type="password" class="form-control" name="confirmPassword" maxlength="50" data-validate="required, length(6-50)">
                        </div>
                    }
                    <div class="form-group">
                        <label for="txtPhoneNumber">تلفن همراه</label>
                        <input id="txtPhoneNumber" type="tel" class="form-control" name="phoneNumber" maxlength="14" data-validate="mobileNumber">
                    </div>
                    <div class="form-group">
                        <label for="txtTelephone">تلفن ثابت</label>
                        <input id="txtTelephone" type="tel" class="form-control" name="telephone" maxlength="14" data-validate="telephone">
                    </div>
                    <div class="form-group">
                        <label for="cmbActive">وضعیت</label>
                        <select id="cmbActive" class="form-control" data-validate="required" name="active">
                            <option value="">...</option>
                            <option value="true">فعال</option>
                            <option value="false">غیرفعال</option>
                        </select>
                    </div>
                    <div class="form-group inline-block w100">
                        <button id="btnNextStep1" type="submit" class="btn btn-primary pull-left">
                            <span>بعدی</span>&nbsp;
                            <i class="fa fa-arrow-left v-middle" aria-hidden="true"></i>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row setup-content" id="step-2">
        <div class="col-xs-12">
            <div class="col-md-12 well">
                <div id="step1" class="col-md-4 col-md-offset-4 text-center">
                    <div class="form-group">
                        <img id="imgUserImage" src="~/content/image/user.png" style="max-width: 200px; border-radius: 50%;" />
                    </div>
                    <div class="form-group">
                        <input id="file" type="file" class="hidden" />
                        <input id="txtFileName" type="text" class="form-control" name="fileName" readonly>
                        <div class="progress">
                            <div class="progress-bar" role="progressbar" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100" style="width:0%">
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <i id="openFile" class="fa fa-upload btn btn-success btn-icon" aria-hidden="true"></i>
                        &nbsp;&nbsp;&nbsp;&nbsp;
                        <i id="deleteFile" class="fa fa-trash btn btn-danger btn-icon" aria-hidden="true"></i>
                    </div>
                    <div class="form-group inline-block w100">
                        <button id="btnPrevStep2" type="submit" class="btn btn-primary pull-right">
                            <i class="fa fa-arrow-right v-middle" aria-hidden="true"></i>&nbsp;
                            <span>قبلی</span>
                        </button>
                        <button id="btnNextStep2" type="submit" class="btn btn-primary pull-left">
                            <span>بعدی</span>&nbsp;
                            <i class="fa fa-arrow-left v-middle" aria-hidden="true"></i>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
